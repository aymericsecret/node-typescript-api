{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [false, "log", "error"],
        "quotemark": [
            true,
            "single"
        ],
        "radix": true,
        "no-shadowed-variable": false,
        "curly": false,
        "max-line-length": false,
        "array-type": false
    },
    "rulesDirectory": []
}